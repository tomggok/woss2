#import "NSObject+DragonTypeConversion.h"

@implementation NSObject (DragonTypeConversion)

- (NSString *)asNSString
{
    if ([self isKindOfClass:[NSString class]])
    {
        return (NSString *)self;
    }else
    {
        return [NSString stringWithFormat:@"%@",self];
    }
}

@end
