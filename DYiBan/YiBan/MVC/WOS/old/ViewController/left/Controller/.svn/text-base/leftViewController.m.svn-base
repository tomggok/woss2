    [bottom setImage:[UIImage imageNamed:@"dotline.png"]];
    [cell addSubview:bottom];
    [bottom release];
    
    [btn addTarget:self action:@selector(showOtherView:) forControlEvents:UIControlEventTouchUpInside];
    [cell addSubview:btn];
    
    [leftList addSubview:cell];
    return cell;
}

